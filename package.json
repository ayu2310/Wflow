{
  "name": "browser-automation-saas",
  "version": "1.0.0",
  "description": "Browser workflow automation SaaS tool with custom user scheduling",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/server/index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "echo 'Backend build complete'",
    "build:frontend": "cd frontend && npm run build",
    "start": "node src/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "bull": "^4.12.2",
    "redis": "^4.6.10",
    "axios": "^1.6.2",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "stagehand": "^0.0.1",
    "playwright": "^1.40.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "browser-automation",
    "saas",
    "scheduling",
    "stagehand",
    "browserbase"
  ],
  "author": "Your Name",
  "license": "MIT"
}